package  {		import flash.utils.IExternalizable;	import flash.net.registerClassAlias;	import flash.utils.IDataInput;	import flash.utils.IDataOutput;	public class RepObject implements IExternalizable	{				public var xVal:Number;		public var yVal:Number;		public var zXVal:Number;		public var zYVal:Number;				public function repObject() 		{			registerClassAlias("RepObject", RepObject);		}		public function writeExternal(output:IDataOutput):void		{			output.writeFloat(xVal);			output.writeFloat(yVal);			output.writeFloat(zXVal);			output.writeFloat(zYVal);		}		public function readExternal(input:IDataInput):void		{			xVal = input.readFloat();			yVal = input.readFloat();			zXVal = input.readFloat();			zYVal = input.readFloat();		}	}	}